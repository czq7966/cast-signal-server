{
  "name": "signal-server",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:cjs": "webpack --env.cjs --env.nodevtool && webpack --env.production --env.cjs ",
    "build:umd": "webpack --env.umd --env.nodevtool && webpack --env.production --env.umd ",
    "watch": "npm run client:clean && webpack --watch --progress --env.cjs",
    "client:clean": "rimraf ./dist/client",
    "client:watch": "npm run client:clean && webpack --watch --progress --env.umd",
    "client:build": "npm run client:clean && webpack --progress --env.umd --env.production",
    "client:compile": "npm run client:clean && webpack --progress --env.umd",
    "dev": "npm run client:watch",
    "server:build": "npm run server:clean && cross-env NODE_MODULE=server webpack --env.cjs --env.production",
    "server:compile": "npm run server:clean && cross-env NODE_MODULE=server webpack --env.cjs",
    "server:clean": "rimraf ./dist/server",
    "server:start": "node ./dist/server/index.js",
    "server": "npm run server:clean && cross-env NODE_MODULE=server webpack --env.cjs && npm run server:start",
    "build": "npm run server:build && npm run client:build",
    "compile": "npm run server:compile && npm run client:compile",
    "start": "npm run server:start "
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@sdp.nd/performancetracker": "^0.1.0",
    "@types/events": "^1.2.0",
    "@types/express": "^4.16.0",
    "@types/node": "^10.12.3",
    "@types/react": "^16.7.6",
    "@types/react-dom": "^16.0.9",
    "@types/socket.io": "^2.1.0",
    "@types/socket.io-client": "^1.4.32",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "es6-object-assign": "^1.1.0",
    "express": "^4.16.4",
    "http": "0.0.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "rimraf": "^2.6.2",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.0",
    "typescript": "^3.1.6",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "uws": "^99.0.0",
    "vconsole": "^3.2.0",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2",
    "webrtc-adapter": "^6.4.6"
  }
}
